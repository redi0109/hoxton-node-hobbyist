// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

model User{
  id Int @id
  fullName String
  photo String @unique
  email String @unique
}

model Hobby{
  id Int @id
  name String
  image String
  active Boolean
  userId Int
}